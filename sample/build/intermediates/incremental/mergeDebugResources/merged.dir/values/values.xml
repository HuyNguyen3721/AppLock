        <attr format="color|reference" name="lock_nodeHighlightSrc"/>
        <attr format="color|reference" name="lock_nodeCorrectSrc"/>
        <attr format="color|reference" name="lock_nodeErrorSrc"/>
        <attr format="color" name="lock_lineColor"/>
        <attr format="color" name="lock_lineCorrectColor"/>
        <attr format="color" name="lock_lineErrorColor"/>
        <attr format="dimension" name="lock_lineWidth"/>
